mixin tagFormat(tag)
    tr
        td
            p=tag.fisher
        td
            p=tag.tag
        td
            img(src=tag.img, width='300px', height='300px')

html(lang="en")
    head
        title FishTags-Test
    body
        table
            tr
                th fisher
                th tag
                th image
            each tag in tags
                +tagFormat(tag)