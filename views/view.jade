mixin tagFormat(tag)
    tr
        td
            p=tag.personInfo
        td
            p=tag.tagInfo
        td
            img(src=tag.photo, width='300px', height='300px')

html(lang="en")
    head
        title FishTags-Test
    body
        table
            tr
                th person Info
                th tag Info
                th photo
            each tag in tags
                +tagFormat(tag)